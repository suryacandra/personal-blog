---
import Layout from "../../components/Layout.astro";
import ListCard from "../../components/ListCard.astro";
const posts = await Astro.glob('../../posts/*.mdx')
---

<Layout title="Latest posts in Ananta Blog" sub="Best article posts in latest" thumb="/favicon.png">
    <ListCard posts={posts.sort((a:any, b:any) => a.frontmatter.id - b.frontmatter.id)} e={false} content='main' />
</Layout>